{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.tsx?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
    "extensions": ["ts", "tsx"],
    "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/jsdoc/templates/default"
  }
}
